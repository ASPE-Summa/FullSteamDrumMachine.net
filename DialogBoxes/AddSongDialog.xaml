<Window x:Class="FullSteamDrumMachine.net.DialogBoxes.AddSongDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FullSteamDrumMachine.net.DialogBoxes"
        mc:Ignorable="d"
        Title="Add a new Song" Height="200" Width="800">
    <StackPanel>
        <TextBlock>Name your new song:</TextBlock>
        <TextBox Text="{Binding SongName}"></TextBox>
        <Button Click="Button_Click">Ok</Button>
    </StackPanel>
</Window>
